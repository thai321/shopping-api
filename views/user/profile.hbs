<h1>{{ userName }}'s Profile</h1>
<div class="row">
    <div class="col-md-8 col-md-offset-2">
      {{#if empty}}
        <h2 class="text-center">{{ userName }} have no order</h2>
      {{ else }}
            {{# each orders }}
                <div class="panel panel-default">
                    <div class="panel-body">
                        <ul class="list-group">
                            {{# each this.items }}
                                <li class="list-group-item">
                                    <span class="badge">{{ this.price }}</span>
                                    {{ this.title }} | {{ this.qty }} Units
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                    <div class="panel-footer">
                        <strong>Total Price: ${{ this.cart.totalPrice }}</strong>
                    </div>
                    <div class="panel-footer">
                        <strong>Quantity: {{ this.cart.totalQuantity }}</strong>
                    </div>
                    <div class="panel-footer">
                        <strong>Date: {{ this.date }}</strong>
                    </div>
                    <div class="panel-footer">
                        <strong>Payment method: {{ this.paymentMethod }}</strong>
                    </div>
                    <div class="panel-footer">
                        <strong>Status: {{ this.status }}</strong>
                    </div>
                </div>

                <hr>
            {{/each}}
        {{/if}}
    </div>
</div>
