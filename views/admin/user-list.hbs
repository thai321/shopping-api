<h1>User List</h1>
{{# each users}}
  <div class="row">
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <div class="caption">
          <h3>Name: {{ this.name }}</h3>
          <h3>Email: {{ this.email }}</h3>
          <h3>phone: {{ this.phone }}</h3>
          <h3>{{ this.carts.length }} carts</h3>
          <h3>{{ this.orders.length }} orders</h3>
        </div>
      </div>
    </div>
  </div>
{{/each}}
